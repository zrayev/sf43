{% extends 'layouts/two-column.html.twig' %}

{% block title %}Company{% endblock %}

{% block content %}
    <h1>Company</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ company.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ company.title }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('company_index') }}">back to list</a>

    <a href="{{ path('company_edit', {'id': company.id}) }}">edit</a>

    <a href="{{ path('company_delete', {'id': company.id}) }}">delete</a>
{% endblock %}
